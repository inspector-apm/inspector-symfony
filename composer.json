{
    "name": "inspector-apm/inspector-symfony",
    "description": "Code Execution Monitoring for Symfony applications.",
    "keywords": ["monitoring", "php", "inspector", "symfony", "bundle"],
    "homepage": "https://www.inspector.dev",
    "license": "MIT",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Valerio Barbera",
            "email": "valerio@inspector.dev"
        },
        {
            "name": "Serhii Smirnov",
            "email": "free.smilesrg@gmail.com"
        }
    ],
    "require": {
		"php": ">=7.2.5",
		"inspector-apm/inspector-php": "^3.8",
        "symfony/config": "^4.4|^5.2|^6.0",
        "symfony/dependency-injection": "^4.4|^5.2|^6.0",
        "symfony/http-kernel": "^4.4|^5.2|^6.0",
        "symfony/event-dispatcher": "^4.4|^5.2|^6.0",
        "symfony/console": "^4.4|^5.2|^6.0",
        "symfony/framework-bundle": "^4.4|^5.2|^6.0",
        "symfony/yaml": "^4.4|^5.2|^6.0",
        "symfony/security-bundle": "^4.4|^5.2|^6.0"
	},
    "require-dev": {
        "doctrine/doctrine-bundle": "^2.4",
        "symfony/framework-bundle": "^4.4|^5.2|^6.0",
        "phpunit/phpunit": "^9.5",
        "symfony/test-pack": "^1.0",
        "symfony/messenger": "^4.4|^5.2|^6.0"
    },
	"autoload": {
        "psr-4": {
            "Inspector\\Symfony\\Bundle\\": "src/"
        }
    },
	"autoload-dev": {
        "psr-4": {
            "Inspector\\Symfony\\Bundle\\Tests\\": "tests/"
        }
    },
	"minimum-stability": "dev",
    "prefer-stable": true
}
